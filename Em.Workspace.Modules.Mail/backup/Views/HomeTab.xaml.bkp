<inf:RibbonTabItem
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:igWpf="http://schemas.infragistics.com/xaml/wpf"
    xmlns:inf="clr-namespace:Em.Workspace.Infrastructure;assembly=Em.Workspace.Infrastructure"        
    x:Class="Em.Workspace.Modules.Mail.Views.HomeTab"             
    Header="Home">
    
    <igWpf:RibbonGroup Caption="Biff">
        <igWpf:ButtonTool Caption="Drafts"
                          Command="{Binding NavigateCommand}"
                          CommandParameter="Drafts"
                          />
    </igWpf:RibbonGroup>
</inf:RibbonTabItem>
